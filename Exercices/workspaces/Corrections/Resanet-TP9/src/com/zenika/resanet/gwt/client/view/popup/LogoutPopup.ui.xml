<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:with type="com.zenika.resanet.gwt.client.i18n.constants.AppConstantes" field="constants"></ui:with>
  <ui:style>
    .yesButton {
    	float: left;
    	margin-left: 15px;
    }
    
    .noButton {
    	float: right;
    	margin-right: 15px;
    }
  </ui:style>
  <g:DialogBox text="{constants.deconnect}">
    <g:FlowPanel>
      <g:Label text="{constants.confirmdeconnect}"></g:Label>
      <g:FlowPanel>
        <g:Button ui:field="yesButton" text="{constants.yes}" addStyleNames="{style.yesButton}" />
        <g:Button ui:field="noButton" text="{constants.no}" addStyleNames="{style.noButton}" />
      </g:FlowPanel>
    </g:FlowPanel>
  </g:DialogBox>

</ui:UiBinder> 